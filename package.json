{
  "name": "buoyfeed",
  "version": "0.0.2",
  "description": "Super fast progressive web app",
  "engines": {
    "node": "7"
  },
  "scripts": {
    "dev": "nodemon -e js,scss -w src -x \"node build && node ./build/server.js\"",
    "start": "node ./build/server.js",
    "build": "node build",
    "test": "standard",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "yarn build && pm2 deploy ecosystem.config.js production"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "4.15.2",
    "material-design-lite": "^1.3.0",
    "mongoose": "^4.11.3",
    "node-fetch": "1.6.3",
    "pm2": "^2.6.1",
    "preact": "8.1.0",
    "preact-redux": "2.0.1",
    "preact-render-to-string": "3.6.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "shrink-ray": "0.1.3",
    "source-map-support": "0.4.15",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "cssnano": "3.10.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "fs-extra-promise": "1.0.1",
    "node-rev": "2.0.0",
    "node-sass": "4.5.2",
    "nodemon": "1.11.0",
    "purify-css": "1.1.9",
    "rollup": "0.41.6",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-json": "2.1.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "1.0.2",
    "standard": "10.0.2",
    "sw-precache": "5.1.1"
  }
}
